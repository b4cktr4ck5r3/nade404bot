generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("LINK_DATABASE_URL")
}

model du_users {
  ID              BigInt @id @default(autoincrement())
  userid          String @db.VarChar(20)
  steamid         String @unique(map: "steamid") @db.VarChar(20)
  member          Int
  last_accountuse Int
}
